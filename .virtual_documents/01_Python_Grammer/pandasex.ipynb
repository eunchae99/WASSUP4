import pandas as pd
import numpy as np

#Series
s = pd.Series([1,2,3,4,5])
print(type(s))
print(s.index)
print(s.dtype)
print(s)

s.index = ['A', 'B', 'C', 'D', 'E']   #인덱스 지정
print(s)


s = pd.Series([1,2,3,4,5], ['A', 'B', 'C', 'D', 'E'])
print(s)
print(s.index)
print(s.values)
print(s['A'])
print(s.A)
print(s[0:5])
print(s['A':'C'])


#Dictionary (Map, JSON) -> Series
